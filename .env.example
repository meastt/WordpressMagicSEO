# WordPress Site Credentials
# Copy this file to .env and fill in your actual credentials
# The .env file is gitignored and will not be committed to version control

# Format: WP_{DOMAIN}_USERNAME and WP_{DOMAIN}_PASSWORD
# Replace dots with underscores in domain names

# Example: griddleking.com
WP_GRIDDLEKING_COM_USERNAME=your_username_here
WP_GRIDDLEKING_COM_PASSWORD=your_app_password_here
WP_GRIDDLEKING_COM_URL=https://griddleking.com
WP_GRIDDLEKING_COM_NICHE=outdoor cooking

# Example: phototipsguy.com
WP_PHOTOTIPSGUY_COM_USERNAME=your_username_here
WP_PHOTOTIPSGUY_COM_PASSWORD=your_app_password_here
WP_PHOTOTIPSGUY_COM_URL=https://phototipsguy.com
WP_PHOTOTIPSGUY_COM_NICHE=photography

# Example: tigertribe.net
WP_TIGERTRIBE_NET_USERNAME=your_username_here
WP_TIGERTRIBE_NET_PASSWORD=your_app_password_here
WP_TIGERTRIBE_NET_URL=https://tigertribe.net
WP_TIGERTRIBE_NET_NICHE=wild cats

# API Keys (if not set elsewhere)
ANTHROPIC_API_KEY=your_anthropic_key_here
GOOGLE_GEMINI_API_KEY=your_gemini_key_here
